{
    "manifest_version": 2,
    "name": "多兔数据魔盘推广插件",
    "version": "1.0.0",
    "content_security_policy": "style-src 'self' 'unsafe-inline';script-src 'self' 'unsafe-eval' https://cdn.bootcss.com; object-src 'self' ;",
    "description": "【多兔数据魔盘推广插件】专业的淘宝客工具。提供优惠券和佣金查询，数据分析等功能。同时针对不同推广场景，提供更多领先解决方案。官网 https://duotu.pro/",
    "icons": {
        "16": "img/16.png",
        "48": "img/48.png",
        "128": "img/128.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "img/16.png",
            "24": "img/24.png",
            "32": "img/32.png"
        },
        "default_title": "多兔数据魔盘推广插件"
        // "default_popup": "html/popup.html"
    },
    "author": "Mr.Wang",
    "background": {
        "persistent": true,
        "scripts": [
            "js/hot-reload.js",
            "js/jquery.js",
            "js/background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*",
                "*://*.duotu.pro/*"
            ],
            "css": [
                "css/contents.css"
            ],
            "js": [
                "js/hot-reload.js",
                "js/jquery.js",
                "js/contents.js"
            ],
            "run_at": "document_end"
        }
    ],
    "offline_enabled": true,
    "options_page": "html/option.html",
    "permissions": [
        "<all_urls>",
        "tabs",
        "history",
        "activeTab",
        "storage",
        "cookies",
        "webRequest",
        "webRequestBlocking",
        "notifications",
        "contextMenus",
        "http://*/*",
        "https://*/*"
    ],
    "content_security_policy": "script-src 'self' https://plugsource.qingtaoke.com; object-src 'self'"
}